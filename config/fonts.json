{
  "fonts": {
    "primary": {
      "name": "Plus Jakarta Sans",
      "weights": [200, 300, 400, 500, 600, 700, 800],
      "googleFontsFamily": "Plus+Jakarta+Sans"
    },
    "fallbacks": [
      {
        "name": "Outfit",
        "weights": [300, 400, 500, 600, 700],
        "googleFontsFamily": "Outfit"
      },
      {
        "name": "Inter",
        "weights": [300, 400, 500, 600, 700],
        "googleFontsFamily": "Inter"
      },
      {
        "name": "Space Grotesk",
        "weights": [300, 400, 500, 600, 700],
        "googleFontsFamily": "Space+Grotesk"
      },
      {
        "name": "Manrope",
        "weights": [300, 400, 500, 600, 700],
        "googleFontsFamily": "Manrope"
      },
      {
        "name": "DM Sans",
        "weights": [300, 400, 500, 600, 700],
        "googleFontsFamily": "DM+Sans"
      }
    ],
    "languages": {
      "japanese": {
        "name": "Noto Sans JP",
        "weights": [300, 400, 500, 600, 700],
        "googleFontsFamily": "Noto+Sans+JP",
        "langCode": "ja"
      }
    },
    "system": [
      "-apple-system",
      "BlinkMacSystemFont",
      "Segoe UI",
      "Roboto",
      "Helvetica Neue",
      "Arial",
      "sans-serif"
    ]
  },
  "fontStacks": {
    "sans": {
      "description": "Primary sans-serif font stack for English/Latin text",
      "stack": [
        "{primary}",
        "{fallbacks}",
        "{languages.japanese}",
        "{system}"
      ]
    },
    "japanese": {
      "description": "Optimized font stack for Japanese text",
      "stack": [
        "{languages.japanese}",
        "{primary}",
        "{system}"
      ]
    }
  },
  "displaySettings": {
    "display": "swap",
    "preconnect": [
      "https://fonts.googleapis.com",
      "https://fonts.gstatic.com"
    ]
  }
}
